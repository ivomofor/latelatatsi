@model IEnumerable<SavedTask>

<div class="panel-body">
    <h2>Task History</h2>
    <table class="table table-sm table-striped table-bordered">
        <thead>
            <tr>
                <th>Result From Task</th>
            </tr>
        </thead>
        <tbody>
            @foreach(SavedTask s in Model)
            {
                <tr>
                    <td>@s.SavedResult</td>
                </tr>
            }
        </tbody>
    </table>
    <div class="text-center">
        <a asp-action="Index" class="btn btn-primary">Return To Task</a>
    </div>
</div>
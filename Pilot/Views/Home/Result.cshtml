@model string


<div class="panel-body">
    <h2>Task Completted</h2>
    <table class="table table-sm table-striped table-bordered">
        <thead>
            <tr>
                <th>Result From Task</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td> @foreach (var s in Model)
                {@s}</td>
            </tr>
        </tbody>
    </table>
    <div class="text-center">
        <a asp-action="Index" class="btn btn-primary">Return To Task</a>
    </div>
</div>
